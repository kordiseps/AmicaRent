@model IEnumerable<WebApplication.DataAccess.viewCariEhliyet>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            Cari Adı Soyadı
        </th>
        <th>
            Veriliş Tarihi
        </th>
        <th>
            Geçerlilik Tarihi
        </th>        
        <th>
            Doğum Yeri
        </th>
        <th>
            Ehliyet Numarası
        </th>
        <th>
            Ehliyet Sınıfı
        </th>
        <th>
            Kan Grubu
        </th>
        <th></th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Cari_AdSoyad)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CariEhliyet_VerilisTarihi)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CariEhliyet_GecerlilikTarihi)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CariEhliyet_DogumYeri)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CariEhliyet_EhliyetNumarasi)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EhliyetSinif_Adi)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.KanGrubu_Adi)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.CariEhliyet_ID }) |
                @Html.ActionLink("Details", "Details", new { id = item.CariEhliyet_ID }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.CariEhliyet_ID })
            </td>
        </tr>
    }

</table>
