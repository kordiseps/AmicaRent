@model IEnumerable<WebApplication.DataAccess.CariSehir>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            Şehir Adı
        </th>
        <th>
            Şehir Durumu
        </th>
        <th>
            Oluşturma Zamanı
        </th>
        <th></th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.CariSehir_Adi)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CariSehir_Status)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CariSehir_CreateDate)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.CariSehir_ID }) |
                @Html.ActionLink("Details", "Details", new { id = item.CariSehir_ID }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.CariSehir_ID })
            </td>
        </tr>
    }

</table>
